@model List<SetorDeCompras.Models.ProdutosModel>

<h2>Lista de Produtos</h2>
<table>
    <thead>
        <tr>
            <th>Nome</th>
            <th>Preço</th>
            <th>Quantidade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var produto in Model)
        {
            <tr>
                <td>@produto.Name</td>
                <td>@produto.Preco</td>
                <td>@produto.Quantidade</td>
            </tr>
        }
    </tbody>
</table>